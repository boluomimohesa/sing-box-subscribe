{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "0.0.0.0:9999",
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "Proxy",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "store_rdrc": true
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "cloudflareDoh",
        "address": "https://1.1.1.1/dns-query",
        "detour": "Proxy"
      },
      {
        "tag": "proxyServerNameServer",
        "address": "https://223.6.6.6/dns-query",
        "detour": "direct"
      },
      {
        "tag": "aliDoh",
        "address": "https://223.5.5.5/dns-query",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "proxyServerNameServer",
        "disable_cache": true
      },
      {
        "clash_mode": "direct",
        "server": "aliDoh"
      },
      {
        "clash_mode": "global",
        "server": "cloudflareDoh"
      },
      {
        "query_type": ["A", "AAAA"],
        "rule_set": "geosite-cn",
        "server": "fakeip"
      },
      {
        "rule_set": "geosite-cn",
        "server": "aliDoh"
      },
      {
        "query_type": ["A", "AAAA"],
        "server": "fakeip"
      }
    ],
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15"
    },
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "0.0.0.0",
      "listen_port": 9650
    }
  ],
  "outbounds": [
    {
      "tag": "Proxy",
      "type": "selector",
      "outbounds": ["auto", "direct", "{all}"]
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": ["{all}"],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "30m",
      "tolerance": 50
    },
    {
      "tag": "üåè cn",
      "type": "selector",
      "outbounds": ["direct", "Proxy"]
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "Proxy",
    "rules": [
      {
        "inbound": "mixed-in",
        "action": "sniff"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "üåè cn"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "Proxy"
      },
      {
        "domain_suffix": [
          ".msftncsi.com",
          ".msftconnecttest.com",
          "boluomimohesa.tk"
        ],
        "domain": [
          "cdn.jsdelivr.net",
          "testingcf.jsdelivr.net",
          "fastly.jsdelivr.net"
        ],
        "outbound": "direct"
      },
      {
        "rule_set": ["geoip-cn", "geosite-cn"],
        "outbound": "üåè cn"
      }
    ],
    "rule_set": [
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/sing/geo/geoip/cn.srs",
        "download_detour": "auto"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/boluomimohesa/sing-rules-dat/sing/bm7/ChinaMax.srs",
        "download_detour": "auto"
      }
    ]
  }
}
